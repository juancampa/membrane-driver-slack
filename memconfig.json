{
  "expressions": {
    "user": {
      "description": "User",
      "type": "text",
      "searchIn": "https://app.slack.com",
      "regex": "data-message-sender=\"([^\"]+)"
    },
    "channel": {
      "description": "Conversation",
      "type": "text",
      "searchIn": "https://app.slack.com",
      "regex": "data-qa-channel-sidebar-channel-id=\"([^\"]+)"
    },
    "message": {
      "description": "Message",
      "type": "text",
      "searchIn": "https://app.slack.com",
      "regex": "data-ts=\"([^\"]+)"
    }
  },
  "dependencies": {
    "http": "http:"
  },
