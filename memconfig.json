{
  "expressions": {
    "user": {
      "description": "User",
      "type": "text",
      "searchIn": "https://app.slack.com",
      "regex": "data-message-sender=\"([^\"]+)"
    },
    "channel": {
      "description": "Conversation",
      "type": "text",
      "searchIn": "https://app.slack.com",
      "regex": "data-qa-channel-sidebar-channel-id=\"([^\"]+)"
    },
    "message": {
      "description": "Message",
      "type": "text",
      "searchIn": "https://app.slack.com",
      "regex": "data-ts=\"([^\"]+)"
    }
  },
  "dependencies": {
    "http": "http:"
  },
  "schema": {
    "types": [
      {
        "name": "Root",
        "description": "Type representing the root of the Slack driver.",
        "actions": [
          {
            "name": "configure",
            "type": "String",
            "params": [
              {
                "name": "apiToken",
                "type": "String"
              }
            ],
            "description": "Configures the Slack API token for authentication and interaction with Slack channels and messages."
          },
          {
            "name": "endpoint",
            "type": "String",
            "hints": {
              "hidden": true
            },
            "params": [
              {
                "name": "method",
                "type": "String"
              },
              {
                "name": "body",
                "type": "String",
                "optional": true
              },
              {
                "name": "path",
                "type": "String"
              },
              {
                "name": "query",
                "type": "String",
                "optional": true
              },
              {
                "name": "headers",
                "type": "String"
              }
            ],
            "description": "Invoked when an HTTP request is received for the Slack driver, allowing interaction with Slack channels and messages."
          }
        ],
        "fields": [
          {
            "name": "status",
            "type": "String",
            "description": "The status of the Slack driver, indicating its current state of operation."
          },
          {
            "name": "channels",
            "type": "ChannelCollection",
            "description": "Collection of channels available in the Slack workspace for interaction and messaging."
          },
          {
            "name": "users",
            "type": "UserCollection",
            "description": "Collection of users available in the Slack workspace for interaction and messaging."
          },
          {
            "name": "parse",
            "type": "List",
            "hints": {
              "hidden": true
            },
            "ofType": {
              "type": "Ref",
              "ofType": "User"
            },
            "params": [
              {
                "name": "name",
                "type": "String"
              },
              {
                "name": "value",
                "type": "String"
              }
            ],
            "description": "List of user-defined parsing rules for processing Slack messages and interactions."
          },
          {
            "name": "tests",
            "type": "Tests",
            "description": "Tests for the functionality and integration of the Slack driver with Slack channels and messages."
          }
        ]
      },
      {
        "name": "ChannelCollection",
        "fields": [
          {
            "name": "one",
            "type": "Channel",
            "params": [
              {
                "name": "id",
                "type": "String"
              }
            ],
            "description": "Retrieves a single channel from the collection based on the provided channel ID."
          },
          {
            "name": "page",
            "type": "ChannelPage",
            "params": [
              {
                "name": "cursor",
                "type": "String",
                "optional": true
              },
              {
                "name": "limit",
                "type": "Int",
                "optional": true
              }
            ],
            "description": "Retrieves a page of channels from the collection, with optional cursor and limit parameters for pagination."
          }
        ],
        "actions": [
          {
            "name": "create",
            "type": "Void",
            "params": [
              {
                "name": "name",
                "type": "String"
              },
              {
                "name": "is_private",
                "type": "Boolean",
                "optional": true
              }
            ],
            "description": "Creates a new channel in the Slack workspace with the specified name and privacy settings."
          }
        ],
        "description": "Collection of channels in the Slack workspace."
      },
      {
        "name": "ChannelPage",
        "fields": [
          {
            "name": "items",
            "type": "List",
            "ofType": "Channel",
            "description": "List of channels included in the current page of the channel collection."
          },
          {
            "name": "next",
            "type": "Ref",
            "ofType": "ChannelPage",
            "description": "Reference to the next page of channels in the channel collection for pagination."
          }
        ],
        "description": "Page of channels in the Slack workspace."
      },
      {
        "name": "Channel",
        "fields": [
          {
            "name": "id",
            "type": "String",
            "description": "The unique identifier of the channel within the Slack workspace."
          },
          {
            "name": "name",
            "type": "String",
            "hints": {
              "primary": true
            },
            "description": "The name of the channel within the Slack workspace."
          },
          {
            "name": "is_channel",
            "type": "Boolean",
            "description": "Indicates whether the channel is a public channel in the Slack workspace."
          },
          {
            "name": "is_group",
            "type": "Boolean",
            "description": "Indicates whether the channel is a private group in the Slack workspace."
          },
          {
            "name": "is_im",
            "type": "Boolean",
            "description": "Indicates whether the channel is a direct message (IM) channel in the Slack workspace."
          },
          {
            "name": "is_mpim",
            "type": "Boolean",
            "description": "Indicates whether the channel is a multi-party IM (MPIM) channel in the Slack workspace."
          },
          {
            "name": "is_private",
            "type": "Boolean",
            "description": "Indicates whether the channel is a private channel in the Slack workspace."
          },
          {
            "name": "created",
            "type": "Int"
          },
          {
            "name": "is_archived",
            "type": "Boolean"
          },
          {
            "name": "is_general",
            "type": "Boolean"
          },
          {
            "name": "unlinked",
            "type": "Int"
          },
          {
            "name": "is_shared",
            "type": "Boolean"
          },
          {
            "name": "is_org_shared",
            "type": "Boolean"
          },
          {
            "name": "is_pending_ext_shared",
            "type": "Boolean"
          },
          {
            "name": "is_read_only",
            "type": "Boolean"
          },
          {
            "name": "is_thread_only",
            "type": "Boolean"
          },
          {
            "name": "is_member",
            "type": "Boolean"
          },
          {
            "name": "is_ext_shared",
            "type": "Boolean"
          },
          {
            "name": "is_non_threadable",
            "type": "Boolean"
          },
          {
            "name": "shared_team_ids",
            "type": "List",
            "ofType": "String"
          },
          {
            "name": "pending_connected_team_ids",
            "type": "List",
            "ofType": "String"
          },
          {
            "name": "previous_names",
            "type": "List",
            "ofType": "String"
          },
          {
            "name": "topic",
            "type": "Topic"
          },
          {
            "name": "purpose",
            "type": "Topic"
          },
          {
            "name": "members",
            "type": "MemberCollection"
          },
          {
            "name": "messages",
            "type": "MessageCollection"
          }
        ],
        "actions": [
          {
            "name": "invite",
            "type": "Void",
            "params": [
              {
                "name": "users",
                "description": "A comma separated list of user IDs. Up to 30 users may be listed.",
                "type": "String"
              }
            ],
            "description": "Invites the specified users to the channel by providing their user IDs in a comma-separated list."
          },
          {
            "name": "sendMessage",
            "type": "Void",
            "params": [
              {
                "name": "text",
                "type": "String"
              },
              {
                "name": "attachments",
                "type": "String",
                "optional": true
              },
              {
                "name": "blocks",
                "type": "String",
                "optional": true
              }
            ],
            "description": "Sends a message to the channel with the specified text, attachments, and blocks for formatting."
          },
          {
            "name": "leave",
            "type": "Void",
            "description": "Leaves the channel, removing the driver from the channel's member list."
          }
        ],
        "events": [
          {
            "name": "onSlashCommand",
            "type": "SlashCommand",
            "description": "Slash command in the Membrane Slack Driver."
          },
          {
            "name": "onEvent",
            "type": "WebhookEvent",
            "description": "Webhook event in the Membrane Slack Driver."
          }
        ],
        "description": "Representation of a Slack channel."
      },
